            function prevTasksFetcher()
            {
                $("#overl").show();
                $("#processTran").show();
                var tMapId1 = "{!seq__Task_Mapper__c.Id}";
                result1 = sforce.connection.query("Select Id, seq__Controlling_Task__c, seq__Controlling_Task_Subject__c, seq__Dependent_Task__c from seq__Controlling_Task_Mapper__c where seq__Controlling_Task__r.seq__Task_Mapper__c = '"+tMapId1+"'");
                records1 = result1.getArray("records");
                for (var i=0; i< records1.length; i++) 
                {
                    var record1 = records1[i];
                    var Con = new Object();
                    Con.controller = record1.seq__Controlling_Task__c;
                    Con.dependent = record1.seq__Dependent_Task__c;
                    sequencedConnections.push(Con);
                    sequencedDeletingMapperIds.push(record1.Id);
                }    
                
                result2 = sforce.connection.query("Select Id, Name, seq__Comment__c, seq__User__c, seq__Group__c, seq__Contact__c, seq__Gap_Period_Days__c, seq__Left_Position__c, seq__Top_Position__c from seq__Task__c where seq__Task_Mapper__c = '"+tMapId1+"'");
                records2 = result2.getArray("records");
                //alert("recTask="+records2);
                var owner1 = "";
                for (var i=0; i< records2.length; i++) 
                {
                    owner1 = "";
                    var record2 = records2[i];
                    var TSeq = new Object();  
                    TSeq.Id = record2.Id;  
                    TSeq.Name = record2.Name;
                    TSeq.Subject = record2.seq__Comment__c;
                    if(record2.seq__Contact__c != undefined && record2.seq__Contact__c != "" && record2.seq__Contact__c != null)    
                        owner1 = "contact";
                    if(record2.seq__User__c != undefined && record2.seq__User__c != "" && record2.seq__User__c != null)
                        owner1 = "user";
                    if(record2.seq__Group__c != undefined && record2.seq__Group__c != "" && record2.seq__Group__c != null)    
                        owner1 = "group";
                    
                    TSeq.Owner = owner1;   
                    if(owner1 == "contact")
                    {
                        TSeq.CId = record2.seq__Contact__c;
                        result11 = sforce.connection.query("Select Id, Name from contact where Id = '"+record2.seq__Contact__c+"'");
                        records11 = result11.getArray("records");
                        //alert("recUser="+records11[0]);
                        TSeq.CName = records11[0].Name;
                    }
                    else
                    {
                        TSeq.CId = "";
                        TSeq.CName = "";
                    }
                    if(owner1 == "user")
                    {
                        TSeq.UId = record2.seq__User__c;
                        result11 = sforce.connection.query("Select Id, Username from User where Id = '"+record2.seq__User__c+"'");
                        records11 = result11.getArray("records");
                        //alert("recUser="+records11[0]);
                        TSeq.UName = records11[0].Username;
                    }
                    else
                    {
                        TSeq.UId = "";
                        TSeq.UName = "";
                    }
                    if(owner1 == "group")
                    {
                        TSeq.GId = record2.seq__Group__c;
                        result11 = sforce.connection.query("Select Id, Name from Group where Id = '"+record2.seq__Group__c+"'");
                        records11 = result11.getArray("records");
                        //alert("recGroup="+records11[0]);
                        TSeq.GName = records11[0].Name;
                    }
                    else
                    {
                        TSeq.GId = "";
                        TSeq.GName = "";
                    }
                    TSeq.Gap = record2.seq__Gap_Period_Days__c; 
                    TSeq.Left = record2.seq__Left_Position__c;
                    TSeq.Top = record2.seq__Top_Position__c;
                    if(sequencedConnections.length == 0)
                    {
                        TSeq.IsStart = 1;
                        TSeq.IsStop = 1;
                    }
                    else
                    {
                        var IsStrt = 1;
                        var IsStp = 1;
                        for(var j = 0; j < sequencedConnections.length; j ++)
                        {
                            if(sequencedConnections[j].controller == record2.Id)
                                IsStp = 0;
                            if(sequencedConnections[j].dependent == record2.Id)
                                IsStrt = 0;    
                        }
                        TSeq.IsStart = IsStrt;
                        TSeq.IsStop = IsStp;
                    }    
                    sequencedWindows.push(TSeq);   
                }
            }
            
            function saveSeq()
            {
                try{
                    if(connections.length > 0)
                    {
                        var isStartIncluded = false;
                        var isEndIncluded = false;
                        var hasOtherStart = false;
                        var hasOtherEnd = false;
                        var hasSrc = false;
                        var hasDest = false;
                        var hasProb = false;
                        
                        doSequenece();
                    
                        for(s1 = 0; s1 < sourceWindows.length; s1 ++)
                        {
                            if(sourceWindows[s1] == "window0")
                            {
                                isStartIncluded = true;
                            }
                            else
                            {
                                hasSrc = false;
                                for(d2 = 0; d2 < destinationWindows.length; d2 ++)
                                {
                                    if(destinationWindows[d2] == sourceWindows[s1])
                                        hasSrc = true;
                                }
                                if(hasSrc == false)
                                {
                                    alert("Task: "+taskInfo[windows.indexOf(sourceWindows[s1])].name+" has no source task.\nOnly Start task should have this property.");
                                    hasProb = true;
                                    return null;
                                }
                            }
                        }
                        if(isStartIncluded == false)
                        {
                            alert("Start task must be included.");
                            hasProb = true;
                            return null;
                        }
                        for(d1 = 0; d1 < destinationWindows.length; d1 ++)
                        {
                            if(destinationWindows[d1] == "window1")
                            {
                                isEndIncluded = true;
                            }
                            else
                            {
                                hasDest = false;
                                for(s2 = 0; s2 < sourceWindows.length; s2 ++)
                                {
                                    //alert("dest: "+destinationWindows[d2]);
                                    if(sourceWindows[s2] == destinationWindows[d1])
                                        hasDest = true;
                                }
                                if(hasDest == false)
                                {
                                    alert("Task: "+taskInfo[windows.indexOf(destinationWindows[d1])].name+" has no next task.\nOnly End task should have this property.");
                                    hasProb = true;
                                    return null;
                                }
                            }                            
                        }
                        if(isEndIncluded == false)
                        {
                            alert("End task must be included.");
                            hasProb = true;
                            return null;
                        }
                        if(hasProb == false)
                        {
                            $("#overl").show();
                            $("#processTran1").show();
                            var tMapId = "{!seq__Task_Mapper__c.Id}";
                            var taskSequences1 = [];
                            var taskSequences2 = [];
                            for(d4 = 0; d4 < sequencingTasks.length; d4 ++)
                            {
                                var taskPos = $("#"+sequencingTasks[d4].IDSeq).position();
                                //alert(sequencingTasks[d4].name+", " + sequencingTasks[d4].contactId +", " + sequencingTasks[d4].userId+", " + sequencingTasks[d4].groupId);
                                if(sequencingTasks[d4].name.toString() != "Start")
                                {
                                    if(sequencingTasks[d4].IDSF == undefined || sequencingTasks[d4].IDSF.toString() == "")
                                    {
                                        //alert(sequencingTasks[d4].name+" = "+sequencingTasks[d4].subject+" = "+tMapId+" = "+taskPos.left);
                                        var task1 = new sforce.SObject("seq__Task__c");
                                        task1.Name = sequencingTasks[d4].name.toString();
                                        //alert("ins: "+sequencingTasks[d4].name.toString());
                                        task1.seq__Comment__c = sequencingTasks[d4].subject.toString();
                                        task1.seq__Task_Mapper__c = tMapId.toString();
                                        task1.seq__Gap_Period_Days__c = parseInt(sequencingTasks[d4].duePeriod);
                                        if(sequencingTasks[d4].groupId == undefined || sequencingTasks[d4].groupId == null || sequencingTasks[d4].groupId == "")
                                            task1.seq__Group__c = null;
                                        else    
                                            task1.seq__Group__c = sequencingTasks[d4].groupId;
                                        if(sequencingTasks[d4].userId == undefined || sequencingTasks[d4].userId == null || sequencingTasks[d4].userId == "")
                                            task1.seq__User__c = null;
                                        else
                                            task1.seq__User__c = sequencingTasks[d4].userId;
                                        if(sequencingTasks[d4].contactId == undefined || sequencingTasks[d4].contactId == null || sequencingTasks[d4].contactId == "")
                                            task1.seq__Contact__c = null;
                                        else
                                            task1.seq__Contact__c = sequencingTasks[d4].contactId; 
                                        task1.seq__Left_Position__c = taskPos.left.toString();
                                        task1.seq__Top_Position__c = taskPos.top.toString();
                                        taskSequences1.push(task1);                               
                                    }
                                    else
                                    {
                                        var task2 = new sforce.SObject("seq__Task__c");
                                        task2.Id = sequencingTasks[d4].IDSF.toString();
                                        task2.Name = sequencingTasks[d4].name.toString();
                                        //alert("upd: "+sequencingTasks[d4].name.toString()+" .. "+taskInfo[windows.indexOf(sequencingTasks[d4].IDSeq.toString())].name);
                                        task2.seq__Comment__c = sequencingTasks[d4].subject.toString();
                                        //task2.seq__Task_Mapper__c = tMapId.toString();
                                        task2.seq__Gap_Period_Days__c = parseInt(sequencingTasks[d4].duePeriod);
                                        if(sequencingTasks[d4].groupId == undefined || sequencingTasks[d4].groupId == null || sequencingTasks[d4].groupId == "")
                                            task2.seq__Group__c = null;
                                        else    
                                            task2.seq__Group__c = sequencingTasks[d4].groupId;
                                        if(sequencingTasks[d4].userId == undefined || sequencingTasks[d4].userId == null || sequencingTasks[d4].userId == "")
                                            task2.seq__User__c = null;
                                        else
                                            task2.seq__User__c = sequencingTasks[d4].userId;
                                        if(sequencingTasks[d4].contactId == undefined || sequencingTasks[d4].contactId == null || sequencingTasks[d4].contactId == "")
                                            task2.seq__Contact__c = null;
                                        else
                                            task2.seq__Contact__c = sequencingTasks[d4].contactId;    
                                        task2.seq__Left_Position__c = taskPos.left.toString();
                                        task2.seq__Top_Position__c = taskPos.top.toString();
                                        taskSequences2.push(task2);
                                    }    
                                }
                            }
                            var allSucs = 0;
                            var allSucs1 = 0;
                            var resultUp = sforce.connection.update(taskSequences2); 
                            var result = sforce.connection.create(taskSequences1);
                            for (var op11=0; op11<resultUp.length; op11++) {
                                if (resultUp[op11].getBoolean("success")) {
                                    allSucs1 ++;
                                }   
                                //alert('resUp '+resultUp[op11]);                           
                            }
                            if(resultUp.length == allSucs1)
                            {
                                for (var op1=0; op1<result.length; op1++) {
                                    if (result[op1].getBoolean("success")) {
                                        allSucs ++;
                                    }  
                                    //alert('res '+result[op1]);                              
                                }
                            }
                            var crtdSuccess = 0;
                            if(result.length == allSucs)
                            {
                                //alert("All Tasks are created successfully.");
                                chkUnload = 1;
                                crtdSuccess = 1;  
                                var delResult = sforce.connection.deleteIds(sequencedDeletingTaskIds);
                                var delResult1 = sforce.connection.deleteIds(sequencedDeletingMapperIds);
                                
                            }    
                            else if(result.length > allSucs && allSucs != 0)
                            {
                                $("#overl").hide();
                                $("#processTran1").hide();
                                alert("Few Tasks are created successfully.");
                                document.location = "/"+tMapId;
                            }    
                            else
                            {
                                $("#overl").hide();
                                $("#processTran1").hide();
                                alert("Task creation failed.");
                                document.location = "/"+tMapId;
                            }
                            /************************* Retrieving created tasks *************************/
                            result3 = sforce.connection.query("Select Name, Id, seq__Comment__c from seq__Task__c where seq__Task_Mapper__c = '"+tMapId+"'");
                            records = result3.getArray("records");
                             
                            if(crtdSuccess == 1)
                            {
                                var taskRelations= [];
                                for(d5 = 0; d5 < sequencingTasks.length; d5 ++)
                                {
                                    var parId = "";
                                    for (var i31=0; i31< records.length; i31++) 
                                    {
                                        var record = records[i31];
                                        if(record.Name == sequencingTasks[d5].name)
                                        {
                                            parId = record.Id;
                                        }   
                                    }
                                    //alert("now: "+sequencingTasks[d5].name.toString());
                                    if(sequencingTasks[d5].name.toString() != "End")
                                    {
                                        for(d6 = 0; d6 < sequencingTasks[d5].childs.length; d6 ++)
                                        {
                                            var chldName = "";
                                            if(taskInfo[windows.indexOf(sequencingTasks[d5].childs[d6])] != undefined)
                                                chldName = taskInfo[windows.indexOf(sequencingTasks[d5].childs[d6])].name;
                                            
                                            var chldId = "";
                                            for (var i32=0; i32< records.length; i32++) 
                                            {
                                                var record1 = records[i32];
                                                if(record1.Name == chldName)
                                                {
                                                    chldId = record1.Id;
                                                }   
                                            }
                                            if(parId.toString() != "" && chldId.toString() != "")
                                            {
                                                //alert(sequencingTasks[d5].name.toString()+" -> "+chldName );
                                                var taskRel = new sforce.SObject("seq__Controlling_Task_Mapper__c");
                                                taskRel.seq__Controlling_Task__c = parId;
                                                taskRel.seq__Dependent_Task__c = chldId;
                                                taskRelations.push(taskRel);   
                                            }    
                                        }
                                    }
                                }
                                var result3 = sforce.connection.create(taskRelations);
                                $("#overl").hide();
                                $("#processTran1").hide();
                                alert("Task Sequencing is successfully processed.");
                                //alert(result3);
                                document.location = "/"+tMapId;
                            } 
                            else
                            {
                                $("#overl").hide();
                                $("#processTran1").hide();
                                alert("Task sequencing process failed.");
                                document.location = "/"+tMapId;
                            }                          
                        }
                    }   
                    else
                    {
                        alert("No connection is there.");
                    }
                }
                catch(e)
                {
                alert(e);
                }   
            }
        </script>
